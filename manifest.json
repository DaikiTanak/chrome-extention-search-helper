{
  "manifest_version": 3,
  "name": "Custom Search Keyword",
  "version": "1.0",
  "description": "Set a custom keyword and search with an additional input.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/icons/256.png",
      "48": "src/icons/256.png",
      "128": "src/icons/256.png"
    }
  },
  "options_page": "src/options/options.html"
}
